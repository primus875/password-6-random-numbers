let alphabet = [ "a","b","c","d","e","f","g","h","i","j","k","l","m","n","o","p","q","r","s","t","u","v","w","x","y","z"]

var a = alphabet[Int.random(in:0...25)]
var b = alphabet[Int.random(in:0...25)]
var c = alphabet[Int.random(in:0...25)]
var d = alphabet[Int.random(in:0...25)]
var e = alphabet[Int.random(in:0...25)]
var g = alphabet[Int.random(in:0...25)]

 let password = a + b + c + d + e + g
print (password)
